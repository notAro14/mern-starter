{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack Boilerplate for React",
  "private": true,
  "scripts": {
    "analyze-build": "webpack --analyze --config webpack/webpack.prod.js",
    "build": "npm-run-all format lint prod",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "format": "yarn run prettier --write .",
    "lint": "eslint --ext .jsx,.js src/ webpack/",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "start": "npm-run-all format lint dev",
    "test": "jest"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "Aro Andriamaro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.5",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": ">=4",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.4.0",
    "webpack-merge": "^5.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": "eslint --cache",
    "*.{js,css,md}": "prettier --write"
  }
}
