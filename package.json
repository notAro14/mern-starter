{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack Boilerplate for React",
  "private": true,
  "scripts": {
    "format": "yarn run prettier --write .",
    "format:check": "yarn run prettier --check .",
    "build": "yarn run format:check & cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build:analyze": "webpack --analyze --config webpack/webpack.prod.js",
    "start": "yarn run format:check & cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Aro Andriamaro",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.5",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.3.2",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
