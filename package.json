{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "MERN starter",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json dist",
    "build": "npm-run-all build:*",
    "build:client": "NODE_ENV=production webpack --config webpack/webpack.prod.js --profile --json > stats.json",
    "build:server": "NODE_ENV=production webpack --config webpack/webpack.server.js",
    "dev:client": "webpack serve --config webpack/webpack.dev.js",
    "dev:server": "NODE_ENV=development webpack --config webpack/webpack.server.js",
    "format": "prettier --write .",
    "lint": "eslint --ext .jsx,.js src/ webpack/",
    "serve": "NODE_ENV=production node dist/server",
    "start": "rm -rf dist/ && npm-run-all --parallel dev:*",
    "test": "jest --coverage --passWithNoTests",
    "update": "yarn upgrade-interactive --latest",
    "postinstall": "husky install"
  },
  "keywords": [
    "boilerplate",
    "mern",
    "mongodb",
    "react",
    "starter",
    "webpack"
  ],
  "author": "Aro Andriamaro <ni.aro.aina@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.10",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "5.1.0",
    "husky": "5",
    "jest": "^26.6.3",
    "lint-staged": ">=10.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.21.2",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "core-js": "3.8.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^1.3.6",
    "mongoose": "^5.11.16",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-shell-plugin-next": "^2.1.1"
  }
}
